      ],
      "source": [
        "def add_all(a, b, *args):\n",
        "    print(args)\n",
        "\n",
        "    sum = 0\n",
        "    for elem in args:\n",
        "        sum += elem\n",
        "\n",
        "    return a + b+ sum\n",
        "\n",
        "print(add_all(1,2,3,4,5))"
